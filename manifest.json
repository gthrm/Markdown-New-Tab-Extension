{
  "manifest_version": 3,
  "name": "Markdown New Tab",
  "version": "1.0.1",
  "description": "A new tab page that acts as a markdown editor for notes.",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "new_tab.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
